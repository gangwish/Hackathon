{
  "manifest_version": 2,
  "name": "Quizlet Extension",
"description": "A Plugin that will create a quizlet page for unknown words",
  "version": "1.0",
  "icons": {
    "128": "Q.png",
    "48": "Q.png",
    "16": "Q.png"
  },
  "browser_action": {
    "default_popup": "popout.html",
    "default_icon": "Q.png"
},
"background":{
  "scripts": ["context.js"],
  "persistent": false
},
"permissions": [
  "contextMenus",
  "storage",
  "notifications",
  "identity",
  "<all_urls>",
  "activeTab",
  "contextMenus",
  "https://sociallanguagelearning.herokuapp.com/*"
],
  "options_page": "options.html",
  "key": ""
}
